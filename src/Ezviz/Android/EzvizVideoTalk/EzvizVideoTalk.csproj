<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0-android</TargetFramework>
		<SupportedOSPlatformVersion>21</SupportedOSPlatformVersion>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<PropertyGroup>
		<Version>1.2.6</Version>
		<Description>Maui binding library for ezviz-videotalk.</Description>
		<PackageId>$(PackageIdPrefix).Android.EzvizVideoTalk</PackageId>
		<Title>$(PackageIdPrefix).Android.EzvizVideoTalk</Title>
		<AssemblyName>$(PackageIdPrefix).Android.$(MSBuildProjectName)</AssemblyName>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="videotalk.aar" />
	</ItemGroup>
</Project>